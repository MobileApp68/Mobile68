{
  "expo": {
    "name": "Stock Valley",
    "slug": "stock-valley",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Stock-Valley.png",
    "scheme": "group68",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB0zx4bbBG5oxixIuUS5VteAcn0a0rwWFM"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Stock-Valley.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Group68"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Stock-Valley.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
